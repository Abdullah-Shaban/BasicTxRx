
set ns [new Simulator]
source tb_compat.tcl

#define server
set serv1 [$ns node]
tb-fix-node $serv1 server1 

#define usrp
set u1 [$ns node]
tb-fix-node $u1 usrp1
$u1 add-desire USRP 1.0

#define link
set link1 [$ns duplex-link $serv1 $u1 1000000.0kb 0.0ms DropTail]
tb-set-ip-link $serv1 $link1 192.168.10.1
tb-set-ip-link $u1 $link1 192.168.10.2
tb-set-netmask $link1 "255.255.255.0"

$ns run
